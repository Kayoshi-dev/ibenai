{
  "name": "apitest",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts test/**/*.test.ts",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch=.ts$ -w -l info -P -p 8000 dist/app.js\""
  },
  "keywords": [],
  "author": "Kayoshi-dev",
  "license": "ISC",
  "dependencies": {
    "fastify-autoload": "^3.10.0",
    "fastify-sensible": "^3.1.2",
    "@fastify/autoload": "^4.0.0",
    "@fastify/cors": "^7.0.0",
    "@fastify/multipart": "^6.0.0",
    "@fastify/sensible": "^4.0.0",
    "@prisma/client": "^3.13.0",
    "fastify": "^3.29.0",
    "fastify-cli": "^2.15.0",
    "fastify-plugin": "^3.0.1",
    "graphql": "^16.4.0",
    "nexus": "^1.3.0",
    "prisma": "^3.13.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.30",
    "@types/tap": "^15.0.7",
    "ts-node": "^10.7.0",
    "concurrently": "^7.1.0",
    "fastify-tsconfig": "^1.0.1",
    "nodemon": "^2.0.16",
    "tap": "^16.1.0",
    "typescript": "^4.6.4"
  }
}